<h1>Add a New Beer</h1><br />

<%= form_for @beer do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :cost %>
    <%= f.number_field :cost %><br>
    <%= f.label :color %>
    <%= f.text_field :color %><br>
    <%= f.label :origin %>
    <%= f.text_field :origin %><br>
    <%= f.submit %>
<% end %>
